<aiml version="2.0">

    <!-- Patterns for AI Courses -->
    <category>
        <pattern>AI GET PREREQUISITES FOR COURSE *</pattern>
        <template>
            <think>
                <set name="domain">AI</set>
                <set name="course"><star/></set>
            </think>
            Fetching prerequisites for AI course having code <star/>.
        </template>
    </category>
    <category>
        <pattern>AI TOTAL CREDIT HOURS IN SEMESTER *</pattern>
        <template>
            <think>
                <set name="domain">AI</set>
                <set name="semester"><star/></set>
            </think>
            Calculating total credit hours for AI semester <star/>.
        </template>
    </category>
    <category>
        <pattern>AI COURSES WITHOUT PREREQUISITES IN SEMESTER *</pattern>
        <template>
            <think>
                <set name="domain">AI</set>
                <set name="semester"><star/></set>
            </think>
            Fetching AI courses without prerequisites in semester <star/>.
        </template>
    </category>
    <category>
        <pattern>AI LIST ALL COURSES IN SEMESTER *</pattern>
        <template>
            <think>
                <set name="domain">AI</set>
                <set name="semester"><star/></set>
            </think>
            Fetching all AI courses for semester <star/>.
        </template>
    </category>
    <category>
        <pattern>AI COMPLETE SEMESTER ROADMAP</pattern>
        <template>
            <set name="domain">AI</set>
            Fetching the complete AI semester roadmap.
        </template>
    </category>

