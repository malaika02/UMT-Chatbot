<!-- Updated for NovaSST chatbot -->

<!-- Bot Name -->
<category>
    <pattern>WHAT IS YOUR NAME</pattern>
    <template>My name is NovaSST</template>
</category>

<category>
        <pattern>TELL ME YOUR NAME</pattern>
        <template>
             <srai>WHAT IS YOUR NAME</srai>
        </template>
    </category>

<!-- Botmaster Name -->
<category>
    <pattern>WHO IS YOUR BOT MASTER</pattern>
    <template>My botmaster is Malaika</template>
</category>

<category>
        <pattern>TELL ME YOUR BOT MASTER NAME</pattern>
        <template>
             <srai>WHO IS YOUR BOT MASTER</srai>
        </template>
</category>

<category>
        <pattern> * BOT MASTER * </pattern>
        <template>
             <srai>WHO IS YOUR BOT MASTER</srai>
        </template>
</category>

<!-- Birthday -->
<category>
    <pattern>WHEN IS YOUR BIRTHDAY</pattern>
    <template>My birthday is December 5, 2024</template>
</category>

<!-- Birthdate Alternative -->
<category>
    <pattern>WHAT IS YOUR BIRTHDATE</pattern>
    <template>I was born on December 5, 2024.</template>
</category>

</aiml>

